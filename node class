// Boris Caraballo Sorting hw
public class Node {
 private int number;
 private Node next;
 public Node()
 {
  number = 0;
  next = null;
 }
 public Node (int number)
 {
  this.number = number;
  next = null;
 }
 public Node (int number, Node next)
 {
  this.number = number;
  this.next = next;
 }
 public void setNext(Node next)
 {
  this.next = next;
 }
 public void setNumber(int number)
 {
  this.number = number;
 }
 public Node getNext()
 {
  return next;
 }
 public int getNumber()
 {
  return number;
 }
}
